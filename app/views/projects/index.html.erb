<h1>All Projects</h1>

<p>
  <%= link_to "Return to home page", root_path, class: "btn btn-primary" %>
  <%= link_to "Add a project", new_project_path, class: "btn btn-success" %>

</p>


<ul>
  <% @projects.each do |project| %>
    <li>
      <%= link_to project.name, project_path(project) %> (<%= project.stage %>)
      </li>
  <% end %>
</ul>

